{
  "name": "gurmukhi-utils",
  "version": "2.1.0",
  "description": "General functions for working with lines of Gurmukhi.",
  "main": "dist/index.min.js",
  "unpkg": "dist/index.min.js",
  "types": "index.d.ts",
  "runkitExampleFilename": "example.js",
  "scripts": {
    "test": "nyc mocha test",
    "publish-coverage": "nyc report --reporter=text-lcov | coveralls",
    "release": "./release.sh",
    "build": "webpack --mode production",
    "build-docs": "jsdoc2md --template README.hbs lib/**/*.js > README.md &&  markdown-toc -i README.md && cp README.md docs/",
    "precommit": "npm run lint && npm run build-docs && git add docs/ README.md",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShabadOS/gurmukhi-utils.git"
  },
  "author": "ShabadOS Team <team@shabados.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ShabadOS/gurmukhi-utils/issues"
  },
  "homepage": "https://github.com/ShabadOS/gurmukhi-utils#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.7",
    "jsdoc-to-markdown": "^5.0.0",
    "markdown-toc": "^1.2.0",
    "mocha": "^5.0.5",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "release-it": "^12.3.5",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  },
  "keywords": [
    "gurmukhi",
    "gurbani",
    "sikhi",
    "transliteration",
    "transliterate",
    "ascii",
    "unicode",
    "gurbaniAkhar",
    "converter"
  ],
  "dependencies": {
    "escape-string-regexp": "^1.0.5"
  }
}
